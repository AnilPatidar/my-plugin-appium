{
  "name": "my-plugin-appium",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "test": "mocha --require ts-node/register -p test/plugin.spec.js --exit --timeout 260000",
    "prepublish": "npx tsc",
    "lint": "eslint '**/*.js' --fix",
    "prettier": "prettier '**/*.js' --write --single-quote",
    "clear-cache": "rm -rf $HOME/.cache/my-plugin-appium-plugin",
    "uninstall": "export APPIUM_HOME=/Users/anil-patidar/.appium && (appium plugin uninstall my-plugin-appium || exit 0)",
    "install-plugin": "export APPIUM_HOME=/Users/anil-patidar/.appium && npm run build && appium plugin install --source=local $(pwd)",
    "reinstall-plugin": "export APPIUM_HOME=/Users/anil-patidar/.appium && (appium plugin uninstall my-plugin-appium || exit 0) && npm run install-plugin",
    "run-server": "export APPIUM_HOME=/Users/anil-patidar/.appium && appium server -ka 800 --use-plugins=my-plugin-appium -pa /wd/hub"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "peerDependencies": {
    "appium": "^2.11.3"
  },
  "appium":{
    "pluginName": "my-plugin-appium",
    "mainClass": "MyPluginAppium",
    "pluginVersion": "1.0.0",
    "pluginType": "appium",
    "pluginPlatform": "android",
    "pluginDescription": "My Appium Plugin",
    "pluginKeywords": ["appium", "plugin"],
    "pluginLicense": "ISC",
    "pluginAuthor": "anil.patidar",
    "pluginHomepage": ""
  },
  "devDependencies": {
    "appium-support": "^2.55.0",
    "@appium/base-plugin": "^2.2.47"
  },
  "Dependencies": {
    "@types/lodash": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  }
}
